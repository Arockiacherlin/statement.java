// random number
import java.util.*;
class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      System.out.println("Enter start num:");
      int st=sc.nextInt();
      System.out.println("Enter last num:");
      int lt=sc.nextInt();
      double rn=st+Math.random()*((st+lt)+1);
      System.out.println(rn);
    }
}

OUTPUT:
Enter start num:
2
Enter last num:
6
4.106330340097312

// ugly number


      
import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner data = new Scanner(System.in);

       

        while (true) {
             System.out.println("Enter number:");
             int n = data.nextInt();
             if (n <= 0) {
                System.out.println("Invalid input");
            } else if (n % 2 == 0 || n % 3 == 0 || n % 5 == 0) {
                System.out.println(n + " is an ugly number");
                
            } else {
                System.out.println(n + " is a strong number");
                
            }
            
        }
    }
}

OUTPUT:
Enter number:
7
7 is a strong number
Enter number:
11
11 is a strong number
Enter number:
8
8 is an ugly number
Enter number:

// sum of numbers

import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner data = new Scanner(System.in);

System.out.println("enter number:");
int n=data.nextInt();

int sum=0;
for (int i = 0; i < n; i++) {
    sum = i + (i + 1);
    System.out.println(sum);
}
        
}
}

OUTPUT:

enter number:
6
1
3
5
7
9
11

// Automorphic number 

import java.util.Scanner;

public class AutomorphicNumberChecker {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a number to check if it's automorphic: ");
        int num = scanner.nextInt();
        int numDigits = 0;
       long divisor = 1;
       long square =num*num;
        
        for (int temp = num; temp > 0; temp /= 10) {
            numDigits++;
        }

        
        for (int i = 0; i < numDigits; i++) {
            divisor *= 10;
        }

        long lastDigitsOfSquare = square % divisor;

     
        if (lastDigitsOfSquare == num) {
            System.out.println(num + " is an automorphic number.");
        } else {
            System.out.println(num + " is not an automorphic number.");
        }

        scanner.close();
    }
}

OUTPUT:

Enter a number : 5 
5 is an automorphic number 
