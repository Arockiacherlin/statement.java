// random number
import java.util.*;
class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      System.out.println("Enter start num:");
      int st=sc.nextInt();
      System.out.println("Enter last num:");
      int lt=sc.nextInt();
      double rn=st+Math.random()*((st+lt)+1);
      System.out.println(rn);
    }
}

OUTPUT:
Enter start num:
2
Enter last num:
6
4.106330340097312

// ugly number


      
import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner data = new Scanner(System.in);

       

        while (true) {
             System.out.println("Enter number:");
             int n = data.nextInt();
             if (n <= 0) {
                System.out.println("Invalid input");
            } else if (n % 2 == 0 || n % 3 == 0 || n % 5 == 0) {
                System.out.println(n + " is an ugly number");
                
            } else {
                System.out.println(n + " is a strong number");
                
            }
            
        }
    }
}

OUTPUT:
Enter number:
7
7 is a strong number
Enter number:
11
11 is a strong number
Enter number:
8
8 is an ugly number
Enter number:
