import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        char[] data={'9','3','5','5','4','8','7','9','6','4','3'};
        System.out.println(data);
        
        String mystring="cherlin";
        mystring.getChars(0,7,data,4);
       System.out.println(data);
    }
}

output:
93554879643
9355cherlin

//

class Main {
    public static void main(String[] args) {
      String a="New york";
      String b=" Hello world";
      String x="java script";
      System.out.println("java script".matches(x));
      System.out.println(" Hello world".matches(b));
      System.out.println(b.regionMatches(7,a,4,5));
    }
}

output:
true
true
false

//

class Main {
    public static void main(String[] args) {
        String x="new world";
        String y=" Hello world";
     
        
        System.out.println(x.replace('e','a'));
        String  a="(?i)Hello";
        System.out.println(y.replaceAll(a,"kavi"));
        String b="Hello";
         System.out.println(y.replaceFirst(b,"hi"));
    }
}

output:
naw world
 kavi world
 hi world

//

class Main {
    public static void main(String[] args) {
        String x="i am luna";
        String y="[,\\.\\s]";
     
        
        
        String[] z=x.split(y);
       for(String s:z){
         System.out.println(s);
    }
}
}

output:
i
am
luna
