import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a string:");
        String y=sc.nextLine();
        char y3=y.charAt(1);
        int y1=y.codePointAt(0);
        int y2=y.codePointBefore(1);
        System.out.println(y3);
        System.out.println(y1);
        System.out.println(y2);
    }
}

OUTPUT:
java
a
106
106

//

import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter string:");
        String name=sc.nextLine();
        System.out.println("enter string:");
        String name1=sc.nextLine();
        
        int y=name.codePointCount(0,3);
        int y1=name.compareTo(name1);
        int y2=name.compareToIgnoreCase(name1);
        System.out.println(y);
        System.out.println(y1);
        System.out.println(y2);
    }

}
OUTPUT:
Enter a string:
java
Enter a string:
java
3
0
0
//
import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter string:");
        String name=sc.nextLine();
        
        
        System.out.println(name.contains("hel"));
        System.out.println(name.contentEquals("hel"));
    }

}

output:
Enter a string:
hel
true
false
